def calculators(x):
    if x.find('+') != -1:
        r = x.split('+')
        otvet = int(r[0]) + int(r[1])
    elif x.find('-') != -1:
        r = x.split('-')
        otvet = int(r[0]) - int(r[1])
    elif x.find('*') != -1:
        r = x.split('*')
        otvet = int(r[0]) * int(r[1])
    elif x.find('/') != -1:
        r = x.split('/')
        otvet = int(r[0]) / int(r[1])
    return otvet
y = (input())
print(calculators(y))
